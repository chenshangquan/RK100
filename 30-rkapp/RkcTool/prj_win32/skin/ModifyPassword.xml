<?xml version="1.0" encoding="utf-8" ?>
<PageModifyPassword>
	<VerticalLayout name="ModifyPasswordlayout" >
	
		<VerticalLayout>
			<Edit float="true" pos="0,0,0,0" width="0" height="0" /><!--无用Edit 用于默认选中-->
			<Label float="true" pos="0,15,0,0" width="100" height="20" text="密码修改" textcolor="#FF242424" font="0" valign="left" gdiplus="false"/>
		
			<Button name="SaveBtn" float="true" pos="760,18,0,0" width="80" height="28" text="保存" font="1" textcolor="ff5f5f5f" hottextcolor="ffffffff" pushedtextcolor="ff007ac0" disabledtextcolor="ffffffff" bkcolor="fecccccc" hotimage="res\public\button\bottonbk.png" pushedbkcolor="feffffff" bordersize="1" bordercolor="fedcdfe1" enabled="false" gdiplus="false"/>
	
			<Label float="true" pos="0,55,0,0" width="100" height="18" text="当前密码" textcolor="#FF4E4E4E" font="1" valign="left" gdiplus="false"/>
			<Edit name="InputCurPwd" float="true" pos="106,54,0,0" width="240" height="24" text="" textcolor="#FF4E4E4E" font="1" valign="left" maxchar="16" password="true" allowedcopy="false" gdiplus="false"/>
			<Button name="CurPwdVisSwitchBtn" float="true" pos="346,54,0,0" width="20" height="20" normalimage="res\public\button\password_mask.png" visible="false" gdiplus="false"/>
			<Label name="PwdIsTrue" float="true" pos="376,54,0,0" width="16" height="16" bkimage="res/public/button/judge_right.png" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsTrue" float="true" pos="401,54,0,0" width="60" height="16" text="密码正确" textcolor="#FF2C8C02" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="PwdIsFalse" float="true" pos="376,54,0,0" width="16" height="16" bkimage="res/public/button/judge_wrong.png" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsFalse" float="true" pos="401,54,0,0" width="60" height="16" text="密码错误" textcolor="#FFD81E06" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsEmpty" float="true" pos="401,54,0,0" width="100" height="16" text="请输入当前密码" textcolor="#FFD81E06" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="Line1" float="true" pos="106,76,0,0" width="260" height="1" bkcolor="#FF8b8b8b" />

			<Label float="true" pos="0,92,0,0" width="100" height="18" text="新密码" textcolor="#FF4E4E4E" font="1" valign="left" />
			<Label name="PwdIsStrong" float="true" pos="377,91,0,0" width="16" height="16" bkimage="res/public/button/password_strength_strong.png" visible="false" gdiplus="false"/>
			<Label name="PwdIsMedium" float="true" pos="377,91,0,0" width="16" height="16" bkimage="res/public/button/password_strength_medium.png" visible="false" gdiplus="false"/>
			<Label name="PwdIsWeak" float="true" pos="377,91,0,0" width="16" height="16" bkimage="res/public/button/password_strength_weak.png" visible="false" gdiplus="false"/>
			<Label name="PwdIsNonStandard" float="true" pos="376,91,0,0" width="16" height="16" bkimage="res/public/button/judge_wrong.png" visible="false" gdiplus="false"/>
			
			<Label name="TextPwdIsStrong" float="true" pos="402,91,0,0" width="16" height="16" text="强" textcolor="#FF2C8C02" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsMedium" float="true" pos="402,91,0,0" width="16" height="16" text="中" textcolor="#FFFFCC00" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsWeak" float="true" pos="402,91,0,0" width="16" height="16" text="弱" textcolor="#FFD81E06" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsNonStandard" float="true" pos="401,91,0,0" width="100" height="16" text="密码不符合要求" textcolor="#FFD81E06" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextNewPwdIsSameAsCurPwd" float="true" pos="401,91,0,0" width="200" height="16" text="新密码与原密码一致，请重新修改" textcolor="#FFD81E06" font="1" valign="left" visible="false" gdiplus="false"/>
			
			<Button name="TextHelp" float="true" pos="435,91,0,0" width="24" height="16" text="帮助" textcolor="#FF53AFE4" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextPwdIsNonStandardTip" float="true" pos="500,91,0,0" width="320" height="16" text="8-16个数字，大小写字母，符号“_”“.”,至少包含2种" textcolor="#FFCCCCCC" font="1" valign="left" visible="false" gdiplus="false"/>
			<Edit name="InputNewPwd" float="true" pos="106,91,0,0" width="240" height="24" text="" textcolor="#FF4E4E4E" font="1" valign="left" maxchar="16" password="true" allowedcopy="false" gdiplus="false"/>
			<Button name="NewPwdVisSwitchBtn" float="true" pos="346,91,0,0" width="20" height="20" normalimage="res\public\button\password_mask.png" visible="false" gdiplus="false"/>
			<Label name="Line2" float="true" pos="106,113,0,0" width="260" height="1" bkcolor="#FF8b8b8b" gdiplus="false"/>
			<Label name="TextInputTip" float="true" pos="106,117,0,0" width="320" height="16" text="8-16个数字，大小写字母，符号“_”“.”,至少包含2种" textcolor="#FFCCCCCC" font="1" valign="left" visible="false" gdiplus="false"/>

			<Label float="true" pos="0,129,0,0" width="100" height="18" text="确认密码" textcolor="#FF4E4E4E" font="1" valign="left" gdiplus="false"/>
			<Edit name="InputCfmPwd" float="true" pos="106,128,0,0" width="240" height="24" text="" textcolor="#FF4E4E4E" font="1" valign="left" maxchar="16" password="true" allowedcopy="false" gdiplus="false"/>
			<Button name="CfmPwdVisSwitchBtn" float="true" pos="346,128,0,0" width="20" height="20" normalimage="res\public\button\password_mask.png" visible="false" gdiplus="false"/>
			<Label name="ConfirmNewPwdPass" float="true" pos="376,128,0,0" width="16" height="16" bkimage="res/public/button/judge_right.png" visible="false" gdiplus="false"/>
			<Label name="ConfirmNewPwdFail" float="true" pos="376,128,0,0" width="16" height="16" bkimage="res/public/button/judge_wrong.png" visible="false" gdiplus="false"/>
			<Label name="TextConfirmNewPwdPass" float="true" pos="402,128,0,0" width="100" height="16" text="密码一致" textcolor="#FF2C8C02" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="TextConfirmNewPwdFail" float="true" pos="402,128,0,0" width="100" height="16" text="密码不一致" textcolor="#FFFF0000" font="1" valign="left" visible="false" gdiplus="false"/>
			<Label name="Line3" float="true" pos="106,150,0,0" width="260" height="1" bkcolor="#FF8b8b8b" gdiplus="false"/>
		</VerticalLayout>
		
		<Case name="casePageInit">				<!--页面初始化-->
			<SaveBtn enabled="false" bkcolor="fecccccc"/>
			<InputCurPwd password="true"/>
			<InputNewPwd password="true"/>
			<InputCfmPwd password="true"/>
			<CurPwdVisSwitchBtn visible="false" normalimage="res\public\button\password_mask.png"/>
			<NewPwdVisSwitchBtn visible="false" normalimage="res\public\button\password_mask.png"/>
			<CfmPwdVisSwitchBtn visible="false" normalimage="res\public\button\password_mask.png"/>
			<Line1 bkcolor="#FF8b8b8b"/>
			<Line2 bkcolor="#FF8b8b8b"/>
			<Line3 bkcolor="#FF8b8b8b"/>
			
			<PwdIsTrue visible="false"/>
			<PwdIsFalse visible="false"/>
			<TextPwdIsTrue visible="false"/>
			<TextPwdIsFalse visible="false"/>
			<TextPwdIsEmpty visible="false"/>
			
			<PwdIsWeak visible="false"/>
			<PwdIsMedium visible="false"/>
			<PwdIsStrong visible="false"/>
			<PwdIsNonStandard visible="false"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="false"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="false"/>
			
			<ConfirmNewPwdPass visible="false"/>
			<ConfirmNewPwdFail visible="false"/>
			<TextConfirmNewPwdPass visible="false"/>
			<TextConfirmNewPwdFail visible="false"/>
		</Case>
		<Case name="caseInputCurPwd">			<!--输入当前密码-->
			<SaveBtn enabled="false" bkcolor="fecccccc"/>
			<CurPwdVisSwitchBtn visible="true" />
			<NewPwdVisSwitchBtn visible="false"/>
			<CfmPwdVisSwitchBtn visible="false"/>
			<Line1 bkcolor="#FF53AFE4"/>
			<Line2 bkcolor="#FF8b8b8b"/>
			<Line3 bkcolor="#FF8b8b8b"/>
			
			<PwdIsTrue visible="false"/>
			<PwdIsFalse visible="false"/>
			<TextPwdIsTrue visible="false"/>
			<TextPwdIsFalse visible="false"/>
			<TextPwdIsEmpty visible="false"/>
			
			<PwdIsWeak visible="false"/>
			<PwdIsMedium visible="false"/>
			<PwdIsStrong visible="false"/>
			<PwdIsNonStandard visible="false"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="false"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="false"/>
			
			<ConfirmNewPwdPass visible="false"/>
			<ConfirmNewPwdFail visible="false"/>
			<TextConfirmNewPwdPass visible="false"/>
			<TextConfirmNewPwdFail visible="false"/>
		</Case>
		<Case name="caseCurPwdIsVisible">		<!--当前密码可见-->
			<InputCurPwd password="false" />
			<CurPwdVisSwitchBtn normalimage="res\public\button\password_plain.png"/>
		</Case>
		<Case name="caseCurPwdIsInvisible">		<!--当前密码不可见-->
			<InputCurPwd password="true" />
			<CurPwdVisSwitchBtn normalimage="res\public\button\password_mask.png"/>
		</Case>
		<Case name="caseCurPwdIsEmpty">			<!--当前密码为空-->
			<PwdIsTrue visible="false"/>
			<PwdIsFalse visible="true"/>
			<TextPwdIsTrue visible="false"/>
			<TextPwdIsFalse visible="false"/>
			<TextPwdIsEmpty visible="true"/>
			<CurPwdVisSwitchBtn visible="false" />
		</Case>
		<Case name="caseIsPwdTrue">				<!--当前密码正确-->
			<PwdIsTrue visible="true"/>
			<PwdIsFalse visible="false"/>
			<TextPwdIsTrue visible="true"/>
			<TextPwdIsFalse visible="false"/>
			<CurPwdVisSwitchBtn visible="false" />
		</Case>
		<Case name="caseIsPwdFalse">			<!--当前密码错误-->
			<PwdIsTrue visible="false"/>
			<PwdIsFalse visible="true"/>
			<TextPwdIsTrue visible="false"/>
			<TextPwdIsFalse visible="true"/>
			<CurPwdVisSwitchBtn visible="false"/>
		</Case>
		<Case name="caseInputNewPwd">			<!--输入新密码-->
			<SaveBtn enabled="false" bkcolor="fecccccc"/>
			<CurPwdVisSwitchBtn visible="false"/>
			<NewPwdVisSwitchBtn visible="true"/>
			<CfmPwdVisSwitchBtn visible="false"/>
			<Line1 bkcolor="#FF8b8b8b"/>
			<Line2 bkcolor="#FF53AFE4"/>
			<Line3 bkcolor="#FF8b8b8b"/>
			
			<PwdIsWeak visible="false"/>
			<PwdIsMedium visible="false"/>
			<PwdIsStrong visible="false"/>
			<PwdIsNonStandard visible="false"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="false"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="true"/>
			
			<ConfirmNewPwdPass visible="false"/>
			<ConfirmNewPwdFail visible="false"/>
			<TextConfirmNewPwdPass visible="false"/>
			<TextConfirmNewPwdFail visible="false"/>
		</Case>
		<Case name="caseNewPwdIsVisible">		<!--新密码可见-->
			<InputNewPwd password="false" />
			<NewPwdVisSwitchBtn normalimage="res\public\button\password_plain.png"/>
		</Case>
		<Case name="caseNewPwdIsInvisible">		<!--新密码不可见-->
			<InputNewPwd password="true" />
			<NewPwdVisSwitchBtn normalimage="res\public\button\password_mask.png"/>
		</Case>
		<Case name="caseNewPwdIsStrong">		<!--新密码强度强-->
			<PwdIsStrong visible="true"/>
			<PwdIsMedium visible="false"/>
			<PwdIsWeak visible="false"/>
			<PwdIsNonStandard visible="false"/>
			<TextPwdIsStrong visible="true"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="true"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="true"/>
		</Case>
		<Case name="caseNewPwdIsMedium">		<!--新密码强度中-->
			<PwdIsStrong visible="false"/>
			<PwdIsMedium visible="true"/>
			<PwdIsWeak visible="false"/>
			<PwdIsNonStandard visible="false"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsMedium visible="true"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="true"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="true"/>
		</Case>
		<Case name="caseNewPwdIsWeak">			<!--新密码强度弱-->
			<PwdIsStrong visible="false"/>
			<PwdIsMedium visible="false"/>
			<PwdIsWeak visible="true"/>
			<PwdIsNonStandard visible="false"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsWeak visible="true"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="true"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="true"/>
		</Case>
		<Case name="caseNewPwdIsNonstandard">	<!--新密码不符合规范-->
			<PwdIsStrong visible="false"/>
			<PwdIsMedium visible="false"/>
			<PwdIsWeak visible="false"/>
			<PwdIsNonStandard visible="true"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsNonStandard visible="true"/>
			<TextNewPwdIsSameAsCurPwd visible="false"/>
			<TextHelp visible="false"/>
			<TextPwdIsNonStandardTip visible="true"/>
			<TextInputTip visible="false"/>
		</Case>
		<Case name="caseNewPwdIsSameAsCurPwd">	<!--新密码与原密码一致-->
			<PwdIsStrong visible="false"/>
			<PwdIsMedium visible="false"/>
			<PwdIsWeak visible="false"/>
			<PwdIsNonStandard visible="true"/>
			<TextPwdIsStrong visible="false"/>
			<TextPwdIsMedium visible="false"/>
			<TextPwdIsWeak visible="false"/>
			<TextPwdIsNonStandard visible="false"/>
			<TextNewPwdIsSameAsCurPwd visible="true"/>
			<TextHelp visible="false"/>
			<TextPwdIsNonStandardTip visible="false"/>
			<TextInputTip visible="false"/>
		</Case>
		<Case name="caseInputCfmNewPwd">		<!--输入确认密码-->
			<SaveBtn enabled="true" bkcolor="feeff2f4"/>
			<CurPwdVisSwitchBtn visible="false"/>
			<NewPwdVisSwitchBtn visible="false"/>
			<CfmPwdVisSwitchBtn visible="true"/>
			<Line1 bkcolor="#FF8b8b8b"/>
			<Line2 bkcolor="#FF8b8b8b"/>
			<Line3 bkcolor="#FF53AFE4"/>
			
			<TextInputTip visible="false"/>
			
			<ConfirmNewPwdPass visible="false"/>
			<ConfirmNewPwdFail visible="false"/>
			<TextConfirmNewPwdPass visible="false"/>
			<TextConfirmNewPwdFail visible="false"/>
		</Case>
		<Case name="caseCfmPwdIsVisible">		<!--确认密码可见-->
			<InputCfmPwd password="false" />
			<CfmPwdVisSwitchBtn normalimage="res\public\button\password_plain.png"/>
		</Case>
		<Case name="caseCfmPwdIsInvisible">		<!--确认密码不可见-->
			<InputCfmPwd password="true" />
			<CfmPwdVisSwitchBtn normalimage="res\public\button\password_mask.png"/>
		</Case>
		<Case name="caseConfirmNewPwdPass">		<!--密码一致-->
			<SaveBtn enabled="false" bkcolor="fecccccc"/>
			<CfmPwdVisSwitchBtn visible="false"/>
			<Line3 bkcolor="#FF8b8b8b"/>
			<ConfirmNewPwdPass visible="true"/>
			<TextConfirmNewPwdPass visible="true"/>
		</Case>
		<Case name="caseConfirmNewPwdFail">		<!--密码不一致-->
			<SaveBtn enabled="false" bkcolor="fecccccc"/>
			<CfmPwdVisSwitchBtn visible="false"/>
			<Line3 bkcolor="#FF8b8b8b"/>
			<ConfirmNewPwdFail visible="true"/>
			<TextConfirmNewPwdFail visible="true"/>
		</Case>
		<Case name="caseSaveBtnDisable">		<!--保存按键禁用-->
			<SaveBtn enabled="false" bkcolor="fecccccc"/>
		</Case>

	</VerticalLayout>
</PageModifyPassword>